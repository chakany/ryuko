{
	"name": "aina",
	"version": "1.0.0",
	"main": "dist/index.js",
	"repository": "https://github.com/jacany/aina",
	"scripts": {
		"start": "cd dist && cross-env NODE_ENV=production node . | npx bunyan -o short",
		"dev": "cd src && npx ts-node-dev --transpile-only . | npx bunyan -o short",
		"dist": "npx sass src/styles:app/static/styles && npx tsc",
		"watch": "npx sass --watch src/styles:app/static/styles",
		"docs": "cd src && npx ts-node-dev --transpile-only docgen.ts | npx bunyan -o short",
		"postinstall": "patch-package"
	},
	"author": "Jack C <30955604+jacany@users.noreply.github.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/bunyan": "^1.8.6",
		"@types/express": "^4.17.11",
		"@types/fast-levenshtein": "^0.0.1",
		"@types/glob": "^7.1.3",
		"@types/node": "^15.0.1",
		"@types/node-schedule": "^1.3.1",
		"cross-env": "^7.0.3",
		"patch-package": "^6.4.7",
		"postinstall-postinstall": "^2.1.0",
		"sass": "^1.32.12",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bootstrap": "^5.0.0",
		"bunyan": "^1.8.15",
		"date-fns": "^2.21.1",
		"discord-akairo": "^8.1.0",
		"discord.js": "^12.5.3",
		"ejs": "^3.1.6",
		"express": "^4.17.1",
		"fast-levenshtein": "^3.0.0",
		"glob": "^7.1.6",
		"mariadb": "^2.5.3",
		"mysql": "^2.18.1",
		"nekos.life": "^2.0.7",
		"node-schedule": "^2.0.0",
		"sequelize": "^6.6.2",
		"shoukaku": "1.5.2",
		"spotify-url-info": "^2.2.0",
		"ytsr": "^3.4.0"
	}
}
